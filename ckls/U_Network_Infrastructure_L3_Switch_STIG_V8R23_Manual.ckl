                    <ATTRIBUTE_DATA>Running and startup configurations are not synchronized.</ATTRIBUTE_DATA>
                </STIG_DATA>
                <STIG_DATA>
                    <VULN_ATTRIBUTE>Rule_ID</VULN_ATTRIBUTE>
                    <ATTRIBUTE_DATA>SV-3072r3_rule</ATTRIBUTE_DATA>
