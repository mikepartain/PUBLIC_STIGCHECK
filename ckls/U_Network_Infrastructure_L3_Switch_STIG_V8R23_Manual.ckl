                </STIG_DATA>
                <STIG_DATA>
                    <VULN_ATTRIBUTE>Rule_ID</VULN_ATTRIBUTE>
                    <ATTRIBUTE_DATA>SV-15313r3_rule</ATTRIBUTE_DATA>
                </STIG_DATA>
