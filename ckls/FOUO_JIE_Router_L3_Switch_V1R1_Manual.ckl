                </STIG_DATA>
                <STIG_DATA>
                    <VULN_ATTRIBUTE>Rule_ID</VULN_ATTRIBUTE>
                    <ATTRIBUTE_DATA>SV-3084r2_rule</ATTRIBUTE_DATA>
                </STIG_DATA>
