                    <ATTRIBUTE_DATA>Network element must only allow SNMP read access.</ATTRIBUTE_DATA>
                </STIG_DATA>
                <STIG_DATA>
                    <VULN_ATTRIBUTE>Rule_ID</VULN_ATTRIBUTE>
                    <ATTRIBUTE_DATA>SV-3969r5_rule</ATTRIBUTE_DATA>
                </STIG_DATA>
