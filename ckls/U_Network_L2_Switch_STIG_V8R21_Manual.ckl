                    <ATTRIBUTE_DATA>The device is not authenticated using a AAA server.</ATTRIBUTE_DATA>
                </STIG_DATA>
                <STIG_DATA>
                    <VULN_ATTRIBUTE>Rule_ID</VULN_ATTRIBUTE>
                    <ATTRIBUTE_DATA>SV-16259r4_rule</ATTRIBUTE_DATA>
                </STIG_DATA>
